{
    "name": "cardei/link-preview",
    "type": "library",
    "description": "Web Installer for Laravel apps",
    "keywords": ["php","installer","laravel"],
    "homepage": "https://github.com/cardei/laravel-web-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "M. Catalin Cardei",
            "email": "m@cardei.studio",
            "homepage": "https://cardei.studio"
        }
    ],
    "require": {
        "php": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Cardei\\LaravelWebInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cardei\\LaravelWebInstaller\\Providers\\LaravelWebInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}